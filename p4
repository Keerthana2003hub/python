#reppetation
message = " warning ! "
print( message*5 ) 
#upper, lower
print(message.upper())
print(message.lower())
#strip
print(message.strip())
#replace
print(message.replace("warning","error"))
name = """keerthi said 'hello'
ram said 'hi'"""
print(name)
print(len(message))
